                            j$('[id$=idPrimeDiv]>div').addClass('has-success has-feedback'); 
                            j$('[id$=idPrimeDiv]>div').removeClass('has-error'); 
                        }else{
                            j$('[id$=idPrimeDiv]>div').addClass('has-error'); 
                            j$('[id$=idPrimeDiv]>div').removeClass('has-success has-feedback'); 
                        }
                }
